{
  "rewrites": [
    {
      "source": "/jekyll/:path*",
      "destination": "/jekyll/:path*/index.html"
    },
    {
      "source": "/jekyll",
      "destination": "/jekyll/index.html"
    },
    {
      "source": "/",
      "destination": "/jekyll/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/jekyll",
      "permanent": true
    }
  ]
}
